def isValid( s: str):

        stack = []
        mapping = {")": "(", "}": "{", "]": "["}
        for char in s:
                if stack:
                    top_element = stack.pop()  
                else:
                    top_element = "A"

                if mapping[char] != top_element:
                    return False
                else:
                    stack.append(char)
        return not stack

        
print(isValid("{]}"))